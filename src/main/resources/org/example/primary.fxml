<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PrimaryController">
   <children>
      <HBox layoutX="10.0" layoutY="111.0" prefHeight="450.0" prefWidth="780.0" spacing="10.0">
         <children>
            <ImageView fx:id="firstImage" fitHeight="350.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
               <viewport>
                  <Rectangle2D />
               </viewport></ImageView>
            <ImageView fx:id="secondImage" fitHeight="350.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
               <viewport>
                  <Rectangle2D />
               </viewport></ImageView>
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </HBox>
      <Button fx:id="firstButton" layoutX="134.0" layoutY="57.0" mnemonicParsing="false" onMouseClicked="#voteOne" prefHeight="26.0" prefWidth="69.0" text="Vote" />
      <Button fx:id="secondButton" layoutX="459.0" layoutY="57.0" mnemonicParsing="false" onMouseClicked="#voteTwo" prefHeight="26.0" prefWidth="69.0" text="Vote" />
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</AnchorPane>
